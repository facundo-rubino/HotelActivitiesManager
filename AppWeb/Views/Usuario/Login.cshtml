@using Dominio

@if (@ViewBag.error != null)
{
    <script>alertify.error('@ViewBag.error')</script>
}

@if (@ViewBag.Mensaje != null)
{

    <script>alertify.success('@ViewBag.Mensaje')</script>

}

<form method="post" action="Login">
    <input type="text" name="email" placeholder="Ingrese email" />
    <input type="password" name="pass" placeholder="Ingrese contraseña" />
    <button type="submit">Login</button>
</form>

<a class="link-secondary" href="/Usuario/CreateHuesped">No tenés usuario? Registrate</a>


